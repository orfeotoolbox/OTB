setlocal

set INCLUDE=%INCLUDE%;@QT5_INCLUDE_PREFIX_NATIVE@;@QT5_INCLUDE_PREFIX_NATIVE@\freetype2
set LIB=%LIB%;@QT5_LIB_PREFIX_NATIVE@
set PATH=@QT5_SB_SRC_NATIVE@\qtbase\bin;@QT5_SB_SRC_NATIVE@\gnuwin32\bin;%PATH%

:: This change of directory is required because qt created some hidden files in current directory
:: and then later refer to them during qtbase build. Without it, configure fails during build.
cd "@QT5_SB_SRC_NATIVE@"
configure.bat @QT5_SB_CONFIG@

endlocal
